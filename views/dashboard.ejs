<form style="width: 80%">
    <div class="form-group">
        <input class="form-control" id="search-text" type="search" placeholder = "filter to search term">
    </div>
    <p>Advanced down arrow</p>
    <button type="submit" class="btn btn-primary" id ="search-btn">Go</button>
</form>





<form>
    <div>
        <label for="search">Name:</label>
        <input type="search" id="search-text" name="search"  />
    </div>
    <div>
        <input type = "checkbox"  value = "Transport" name = "deptOptions"> 
        Transport <br>
        <input type = "checkbox"  value = "Health" name = "deptOptions"> 
        Health <br>
    </div>
    <div>
        <label for="fromDate">From:</label>
        <input type="date" ide = "fromDate">
    </div>
        <div>
        <label for="toDate">To:</label>
        <input type="date" id = "toDate">
    </div>
    <div>
        <label for="tMember">Tabling member</label>
        <input type="text" id="tMember" list="memberList" />
        <datalist id="memberList">
          <option>Apple</option>
          <option>Banana</option>
          <option>Blackberry</option>
          <option>Blueberry</option>
          <option>Lemon</option>
          <option>Lychee</option>
          <option>Peach</option>
          <option>Pear</option>
        </datalist>
    </div>
    <div>
        <fieldset>
          <legend>House?</legend>
          <p><label for="g1"><input type="radio" name="g" id="g1" value="Both"> Both</label></p>
          <p><label for="g2"><input type="radio" name="g" id="g2" value="Commons"> Commons</label></p>
          <p><label for="g3"><input type="radio" name="g" id="g3" value="Lords"> Lords</label></p>
        </fieldset>
        
    </div>
    
    <div>
        <button type="button" id ="search-btn">Submit</button>
    </div>
</form>

<div id="chartDepartment" style="width:75%; height:400px;"></div>
<br>
<div id="chartHouse" style="width:75%; height:400px;"></div>
<br>
